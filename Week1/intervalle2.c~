#include <stdio.h>

int main(void)
{
    printf("Entez un réel : ");
    double x = 0.0;
    scanf("%lf", &x);
    if ((!(x < 2) && x <3) || ((!(x<0) && !(x==0)) && (x<1 || x ==1))  || (!(x<-10) && (x<-2 || x ==-2))){
        printf("x appartient à I");
    } else {
        printf("x n'appartient pas à I");
    }
    return 0;
}
